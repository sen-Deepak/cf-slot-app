{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "vanilla",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*\\.html?)$",
      "dest": "/public/$1"
    },
    {
      "src": "/.*",
      "dest": "/public/login.html"
    }
  ]
}
